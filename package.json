{
  "name": "command-line-args",
  "version": "6.0.0",
  "description": "A mature, feature-complete library to parse command-line options.",
  "author": "Lloyd Brookes <opensource@75lb.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/75lb/command-line-args.git"
  },
  "homepage": "https://github.com/75lb/command-line-args",
  "bugs": {
    "url": "https://github.com/75lb/command-line-args/issues"
  },
  "scripts": {
    "test": "npm run dist && npm run test:ci",
    "test:ci": "75lb-nature test-runner test/*.js",
    "dist": "75lb-nature cjs-build index.js"
  },
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./fromTo": "./lib/from-to.js"
  },
  "keywords": [
    "argv",
    "parse",
    "argument",
    "args",
    "option",
    "options",
    "parser",
    "parsing",
    "cli",
    "command",
    "line"
  ],
  "engines": {
    "node": ">=12.20"
  },
  "peerDependencies": {
    "@75lb/nature": "latest"
  },
  "peerDependenciesMeta": {
    "@75lb/nature": {
      "optional": true
    }
  },
  "files": [
    "index.js",
    "lib",
    "dist"
  ],
  "standard": {
    "ignore": [
      "dist",
      "tmp"
    ]
  },
  "dependencies": {
    "array-back": "^6.2.2"
  }
}
